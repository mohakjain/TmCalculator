<!--/******************************************************************************/
    	/* contains the parameters for RNA/RNA single mismatches of ;
	REF: Brent M Znosko et al (2007). Biochemistry 46: 13425-13436. 
    	/******************************************************************************/-->
   			
   		<data type="single_mismatch">
   			<parameters sequence="A/G">
	        	<enthalpy>-6900.0</enthalpy>
                <entropy>-21.0</entropy>
   			</parameters>	
   			<parameters sequence="G/G">
	        	<enthalpy>-18200.0</enthalpy>
                <entropy>-51.8</entropy>
   			</parameters>			
   			<parameters sequence="U/U">
	        	<enthalpy>-12600.0</enthalpy>
                <entropy>-39.6</entropy>
   			</parameters>
   			
   			<mismatch sequence="YRR/RRY">
	        	<enthalpy>200.0</enthalpy>
                <entropy>-1.4</entropy>
   			</mismatch>
  			<mismatch sequence="RYY/YYR">
	        	<enthalpy>-5500.0</enthalpy>
                <entropy>-16.25</entropy>
   			</mismatch>
  			<mismatch sequence="YYR/RYY">
	        	<enthalpy>2200.0</enthalpy>
                <entropy>5.7</entropy>
   			</mismatch>
  			<mismatch sequence="YRY/RYR">
	        	<enthalpy>-7700.0</enthalpy>
                <entropy>-23.4</entropy>
   			</mismatch>
  			<mismatch sequence="RRY/YYR">
	        	<enthalpy>-19500.0</enthalpy>
                <entropy>-59.7</entropy>
   			</mismatch>
   			
  			<closure type="per_A/U">
	        	<enthalpy>-1800.0</enthalpy>
                <entropy>-9.8</entropy>
   			</closure>
   			<closure type="per_G/U">
	        	<enthalpy>-1800.0</enthalpy>
                <entropy>-9.8</entropy>
   			</closure>
   		</data>