<!--/******************************************************************************/
    	/* contains the parameters for RNA/RNA single mismatches of ;
	REF: Brent M Znosko et al (2008). Biochemistry 47: 10178-10187. 
    	/******************************************************************************/-->
   			
   		<data type="single_mismatch">
   			<parameters sequence="A/G">
	        	<enthalpy>-800.0</enthalpy>
                <entropy>-10.4</entropy>
   			</parameters>	
   			<parameters sequence="G/G">
	        	<enthalpy>-17900.0</enthalpy>
                <entropy>-52.2</entropy>
   			</parameters>			
   			<parameters sequence="U/U">
	        	<enthalpy>-14900.0</enthalpy>
                <entropy>-45.7</entropy>
   			</parameters>
   			
   			<mismatch sequence="YRR/RRY">
	        	<enthalpy>800.0</enthalpy>
                <entropy>6.2</entropy>
   			</mismatch>
  			<mismatch sequence="RYY/YYR">
	        	<enthalpy>-300.0</enthalpy>
                <entropy>-1.2</entropy>
   			</mismatch>
  			<mismatch sequence="YYR/RYY">
	        	<enthalpy>2600.0</enthalpy>
                <entropy>6.5</entropy>
   			</mismatch>
  			<mismatch sequence="YRY/RYR">
	        	<enthalpy>-7000.0</enthalpy>
                <entropy>-20.3</entropy>
   			</mismatch>
  			<mismatch sequence="RRY/YYR">
	        	<enthalpy>-17300.0</enthalpy>
                <entropy>-54.5</entropy>
   			</mismatch>
   			
  			<closure type="per_A/U">
	        	<enthalpy>-4000.0</enthalpy>
                <entropy>-15.0</entropy>
   			</closure>	
  			<closure type="per_G/U">
	        	<enthalpy>-4000.0</enthalpy>
                <entropy>-19.3</entropy>
   			</closure>
   		</data>